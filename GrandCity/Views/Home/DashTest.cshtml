<script>
    let draggies = [];
    let ___allLoaded___ = 0;
    let maxRepLoadCount = 12;
    function initRearrangerBase() {
        ___allLoaded___++;
        if (___allLoaded___ >= maxRepLoadCount) {

            $('.drag-hero-demos__grid').packery({
                // options
                itemSelector: '.drag-hero-demos__item',
                gutter: 10
            });

            $('.drag-hero-demos__grid').find('.drag-hero-demos__item').each(function (i, gridItem) {
                var draggie = new Draggabilly(gridItem);
                draggies.push(draggie);
                // bind drag events to Packery
                $('.drag-hero-demos__grid').packery('bindDraggabillyEvents', draggie);
            });
        }
    }

    $(function () {
        //showing a loader on all report places
        SASLoad($("#blk-recovery"));
        SASLoad($("#instrep"));
        SASLoad($("#revreport"));
        SASLoad($("#paymentreport"));
        SASLoad($("#transferplot"));
        SASLoad($("#bookin"));
        SASLoad($("#sc-short"));
        SASLoad($("#ec-short"));
        SASLoad($("#file-year"));
        SASLoad($("#att-rep-data"));
        SASLoad($("#payrl-rep-data"));
        SASLoad($("#sal-rep-data"));
    });

    //document Ready function...
    $(document).ready(function () {
        var date = "@DateTime.Now";
        $("#blk-recovery").load('/Reports/PlotsRecovery/', { From: null, To: null }, function () {
            initRearrangerBase();
            SASUnLoad($("#blk-recovery"));
        });
        $("#instrep").load('/Reports/MonthlyInstallment/', { From: date, To: date }, function () {
            SASUnLoad($("#instrep"));
            initRearrangerBase();
        });
        $("#revreport").load('/Reports/RevenueReport/', { From: date, To: date }, function () {
            SASUnLoad($("#revreport"));
            initRearrangerBase();
        });
        $("#paymentreport").load('/Reports/PayableReport/', { From: date, To: date }, function () {
            SASUnLoad($("#paymentreport"));
            initRearrangerBase();
        });
        $("#transferplot").load('/Reports/PlotTranfer/', { Startdate: date, Enddate: date }, function () {
            SASUnLoad($("#transferplot"));
            initRearrangerBase();
        });
        $("#bookin").load('/Reports/PlotBooking/', { Startdate: date, Enddate: date }, function () {
            SASUnLoad($("#bookin"));
            initRearrangerBase();
        });
        $("#sc-short").load('/Reports/ServiceChargesBills_Short/', function () {
            SASUnLoad($("#sc-short"));
            initRearrangerBase();
        });
        $("#ec-short").load('/Reports/ElectricityChargesBill_Short/', function () {
            SASUnLoad($("#ec-short"));
            initRearrangerBase();
        });
        $("#file-year").load('/Reports/FilesYearlyRecovery/', function () {
            SASUnLoad($("#file-year"));
            initRearrangerBase();
        });
        $("#att-rep-data").load('/Reports/AttendanceReport/', function () {
            SASUnLoad($("#att-rep-data"));
            initRearrangerBase();
        });
        $("#payrl-rep-data").load('/Reports/PayrollReport/', function () {
            SASUnLoad($("#payrl-rep-data"));
            initRearrangerBase();
        });
        $("#sal-rep-data").load('/Reports/SalaryComparativeReport?typ=AllComps', function () {
            SASUnLoad($("#sal-rep-data"));
            initRearrangerBase();
        });
    });

    $('.st-dt-khjdf').bootstrapMaterialDatePicker({ weekStart: 0, time: false, format: 'MM-DD-YYYY' }).on('change', function (e, date) {
        $('.en-dt-khjdf').bootstrapMaterialDatePicker('setMinDate', date);
    });
    $('.en-dt-khjdf').bootstrapMaterialDatePicker({ weekStart: 0, time: false, format: 'MM-DD-YYYY' });

    $('.st-dt-dfhjgs').bootstrapMaterialDatePicker({ weekStart: 0, time: false, format: 'MM-DD-YYYY' }).on('change', function (e, date) {
        $('.en-dt-fkjdghs').bootstrapMaterialDatePicker('setMinDate', date);
    });
    $('.en-dt-fkjdghs').bootstrapMaterialDatePicker({ weekStart: 0, time: false, format: 'MM-DD-YYYY' });

    $('.st-dt-skdfj-sdfhjg').bootstrapMaterialDatePicker({ weekStart: 0, time: false, format: 'MM-DD-YYYY' }).on('change', function (e, date) {
        $('.en-dt-hsfdgvhjs-asdhjgv').bootstrapMaterialDatePicker('setMinDate', date);
    });
    $('.en-dt-hsfdgvhjs-asdhjgv').bootstrapMaterialDatePicker({ weekStart: 0, time: false, format: 'MM-DD-YYYY' });

