        let depNam = $.trim($('.dep-att-lst-gskfskj').text());
        window.open('/Reports/DetailedAttendanceReport?start=' + stDt + '&end=' + enDt + '&depart=' + dep + '&typ=' + depNam);
    });

    $('.dets-payrl-rep-btn-fdljk').unbind().click(function () {
        let stDt = $('.st-dt-dfhjgs').val();
