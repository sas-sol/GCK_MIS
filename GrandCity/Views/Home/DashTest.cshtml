        let stDt = $('.st-dt-khjdf').val();
        let enDt = $('.en-dt-khjdf').val();
        let dep = $('.dep-att-lst-gskfskj option:selected').val();

        $('#att-rep-data').load('/Reports/AttendanceReport/', { start: stDt, end: enDt, depart: dep });
    });

    $('.get-payrl-rep-btn-fdljk').unbind().click(function () {
        let stDt = $('.st-dt-dfhjgs').val();
        let enDt = $('.en-dt-fkjdghs').val();
