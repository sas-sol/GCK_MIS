    });

    $('.fhjkasd').unbind().click(function () {
        if (!SASLoaded($(this))) {
            SASLoad($(this));
        }
        else {
            SASUnLoad($(this));
        }
    });

    $('.edt-btn').unbind().click(function () {
        if ($(this).hasClass('__edit___Mode__')) {
            $('.drag-hero-demos__grid').packery('destroy');
            $.each(draggies, function (i, v) {
                v.destroy();
            });
            draggies = [];
            $(this).removeClass('__edit___Mode__');
        }
        else {
            $(this).addClass('__edit___Mode__');
            $('.drag-hero-demos__grid').find('.drag-hero-demos__item').each(function (i, gridItem) {
                var draggie = new Draggabilly(gridItem);
                draggies.push(draggie);
                // bind drag events to Packery
                $('.drag-hero-demos__grid').packery('bindDraggabillyEvents', draggie);
            });
        }
    });

</script>

