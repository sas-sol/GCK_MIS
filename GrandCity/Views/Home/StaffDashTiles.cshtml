<style>
    .a {
        color: #592a88;
    }

    .hexagon a {
        font-size: 10px;
        font-weight: bold;
    }

    .text {
        position: absolute;
        z-index: 1000;
        width: 100%;
        text-align: center;
        font-size: 17px;
    }

    .hexagon {
        position: relative;
        width: 160px;
        height: 90px;
        border-left: solid 2px transparent;
        border-right: solid 2px transparent;
        cursor: pointer;
        padding-top: 10px;
    }

        .hexagon:before,
        .hexagon:after {
            content: "";
            position: absolute;
            z-index: 1;
            width: 113px;
            height: 113px;
            -webkit-transform: scaleY(0.5774) rotate(-45deg);
            -ms-transform: scaleY(0.5774) rotate(-45deg);
            transform: scaleY(0.5774) rotate(-45deg);
            background-color: inherit;
