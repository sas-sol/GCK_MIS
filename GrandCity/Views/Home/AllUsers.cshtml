@model IEnumerable<MeherEstateDevelopers.Models.Sp_Get_Users_Result>
@{ Layout = "~/Views/Shared/_AdminLayout.cshtml"; }
<div class="row">
    <div class="col-md-12">
        <div class="bgc-white bd bdrs-3 p-20 mB-20">
            <h4 class="c-grey-900 mB-20">Users</h4>
            <table id="dataTable" class="table table-striped table-bordered" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>CNIC</th>
                        <th>Address</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        if (Model.Any())
                        {
                            foreach (var item in Model)
                            {
                                <tr class="pointer" id="@item.Id">
                                    <td>@item.Name</td>
                                    <td>@item.Email</td>
                                    <td>@item.CNIC</td>
                                    <td>@item.Address</td>
                                </tr>
                            }
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>