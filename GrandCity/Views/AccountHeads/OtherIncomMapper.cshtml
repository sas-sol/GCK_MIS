@model IEnumerable<MeherEstateDevelopers.Models.COA_Mapper>
@using MeherEstateDevelopers.Models;
@{
    var a = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Plot_Sales_Commission_Expense.ToString()).FirstOrDefault();
    var b = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Fines_And_Penalties.ToString()).FirstOrDefault();
    var c = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Reinstate_Charges.ToString()).FirstOrDefault();
    var d = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Posession_Charges.ToString()).FirstOrDefault();
    var e = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Duplicate_Allotment_Letter.ToString()).FirstOrDefault();
    var f = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Registry_Charges.ToString()).FirstOrDefault();
    var g = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Power_Of_Attorney.ToString()).FirstOrDefault();
    var h = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Out_Station_Charges.ToString()).FirstOrDefault();
    var i = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Other_Recovery.ToString()).FirstOrDefault();
    var j = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Duplicate_Customer_File.ToString()).FirstOrDefault();
    var k = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Other_Transfer_Charges.ToString()).FirstOrDefault();
    var l = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Advance_Tax_236_C.ToString()).FirstOrDefault();
    var m = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.Advance_Tax_236_K.ToString()).FirstOrDefault();
    var n = Model.Where(x => x.ModuleType == COA_Mapper_ModuleTypes.New_Connection_Charges.ToString()).FirstOrDefault();
