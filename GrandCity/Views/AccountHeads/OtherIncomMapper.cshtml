                            <option value="@m.AccountId">@m.AccountCode - @m.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>New Connection Charges</h5>
                    <select class="form-control sel-plug n">
                        @if (n != null)
                        {
                            <option value="@n.AccountId">@n.AccountCode - @n.Head_Name</option>
                        }
                    </select>
                </div>

                <div class="form-group col-md-12">
                    <button class="btn btn-primary up-inco">Update</button>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $('.sel-plug').select2({
        placeholder: "Search for Account",
        ajax: {
            url: '/AccountHeads/GetHead/',
            dataType: 'json',
            params: {
                contentType: 'application/json; charset=utf-8'
            },
            data: function (params) {
                return {
                    q: params.term// search term
                };
            },
            processResults: function (data) {
                // Transforms the top-level key of the response object from 'items' to 'results'
                return {
                    results: $.map(data.items, function (item) {
                        return {
                            text: item.Parent,
                            children: [{
                                id: item.id,
                                text: item.text
                            }]
                        }
                    })
                };
            }
        }
    });



</script>