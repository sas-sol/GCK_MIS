    <div class="form-row row">
        <div class="col-md-12">
            <div class="from-row row p-20">
                <div class="form-group col-md-3">
                    <h5>Plot_Sales_Commission_Expense</h5>
                    <select class="form-control sel-plug a">
                        @if (a != null)
                        {
                            <option value="@a.AccountId">@a.AccountCode - @a.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>Fines_And_Penalties</h5>
                    <select class="form-control sel-plug b">
                        @if (b != null)
                        {
                            <option value="@b.AccountId">@b.AccountCode - @b.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>Reinstate_Charges</h5>
                    <select class="form-control sel-plug c">
                        @if (c != null)
                        {
                            <option value="@c.AccountId">@c.AccountCode - @c.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>Posession_Charges</h5>
                    <select class="form-control sel-plug d">
                        @if (d != null)
                        {
                            <option value="@d.AccountId">@d.AccountCode - @d.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>Duplicate_Allotment_Letter</h5>
                    <select class="form-control sel-plug e">
                        @if (e != null)
                        {
                            <option value="@e.AccountId">@e.AccountCode - @e.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>Registry_Charges</h5>
                    <select class="form-control sel-plug f">
                        @if (f != null)
                        {
                            <option value="@f.AccountId">@f.AccountCode - @f.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>Power_Of_Attorney</h5>
                    <select class="form-control sel-plug g">
                        @if (g != null)
                        {
                            <option value="@g.AccountId">@g.AccountCode - @g.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>Out_Station_Charges</h5>
                    <select class="form-control sel-plug h">
                        @if (h != null)
                        {
                            <option value="@h.AccountId">@h.AccountCode - @h.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>Other_Recovery</h5>
                    <select class="form-control sel-plug i">
                        @if (i != null)
                        {
                            <option value="@i.AccountId">@i.AccountCode - @i.Head_Name</option>
                        }
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <h5>Duplicate_Customer_File</h5>
                    <select class="form-control sel-plug j">
                        @if (j != null)
                        {
                            <option value="@j.AccountId">@j.AccountCode - @j.Head_Name</option>
