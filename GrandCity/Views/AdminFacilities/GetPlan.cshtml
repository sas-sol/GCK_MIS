@model IEnumerable<MeherEstateDevelopers.Models.Sp_Get_EntertaimentFeePlan_Result>
@{
    if (Model.Any())
    {
        foreach (var item in Model.GroupBy(x => new { x.Project_Name, x.Project_Id }))
        {
            <div class="col-md-4">
                <div class="bgc-white p-20 bd">
                    <i class="ti-trash pointer del__ad__stru" id="@item.Key.Project_Id" style="float:right"></i>
                    @*<i class="ti-pencil pointer upd__ad__stru" id="@item.Key.Project_Id" style="float:right"></i>*@
                    <i class="ti-pencil upd__ad__stru pointer" id="@item.Key.Project_Id" data-toggle="modal" data-target="#Modal" style="float:right"></i>
                    <h5>Name: @item.Key.Project_Name</h5>
                    <hr />
                    @foreach (var i in item)
                    {
                        <h6>Employee Membership Fee : <span style="float:right;font-weight:normal">@string.Format("{0:n0}", i.Emp_Mem_Fee)   </span></h6>
                        <h6>Employee Monthly Fee : <span style="float:right;font-weight:normal"> @string.Format("{0:n0}", i.Emp_Fee)</span></h6>
                        <h6>Resident Membership Fee : <span style="float:right;font-weight:normal">@string.Format("{0:n0}", i.Res_Mem_Fee)</span></h6>
                        <h6>Resident Monthly Fee : <span style="float:right;font-weight:normal">@string.Format("{0:n0}", i.Res_Fee)</span></h6>
                        <h6>Outsider Membership Fee : <span style="float:right;font-weight:normal">@string.Format("{0:n0}", i.Out_Mem_Fee)</span></h6>
                        <h6>Outsider Monthly Fee : <span style="float:right;font-weight:normal">@string.Format("{0:n0}", i.Out_Fee)</span></h6>
                    }
                </div>
            </div>
        }
    }
}

