@{
    Layout = "~/Views/Shared/_StaffLayout.cshtml";
}
<script src="https://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>
<script src="https://code.highcharts.com/modules/export-data.js"></script>
<div class="row gap-20 pos-r">
    <div class="container-fluid">
        <h4 class="c-grey-900 mT-10 mB-30"></h4>
        <ul class="nav nav-tabs" id="myTabs">
            @{ string activeclass = "active";}
            <li class="nav-item"><a class="nav-link @activeclass" data-toggle="tab" role="tab" data-link="/Reports/SAMLeadsReports" href="#cheques">SA Marketing</a></li>
            @{ activeclass = ""; }
            <li class="nav-item"><a class="nav-link @activeclass" data-toggle="tab" role="tab" data-link="/Reports/PropertyExchangeLeadsReports" href="#cheques">Property Exchange</a></li>
        </ul>
        <!-- Tab panes -->
        <div class="tab-content">
            <div id="cheques" class="tab-pane active">
                @{ Html.RenderAction("SAMLeadsReports", "Reports");}
            </div>
        </div>
    </div>
</div>