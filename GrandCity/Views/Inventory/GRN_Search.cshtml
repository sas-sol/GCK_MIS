@model IEnumerable<MeherEstateDevelopers.Models.Sp_Get_GRN_List_Result>
<table id="inv" class="table table-striped table-bordered " cellspacing="0" width="100%" style="margin-left:0%">
    <thead class="thead-dark">
        <tr>
            <th>Sr.</th>
            <th>GRN No</th>
            <th>GRN Date</th>
            <th>Department</th>
            <th>Generated By</th>
            @*<th>Total Amount</th>*@
            <th>Received Items</th>
        </tr>
    </thead>
    <tbody>
        @{
            if (Model.Any())
            {
                int i = 1;
                foreach (var g in Model.GroupBy(x => new { x.GRN, x.DATETIME, x.Prj_Name, x.Dep_Name, x.CreatedBy_Name}))
                {
        <tr class="pointer">
            <td>@i</td>
            <td>@g.Key.GRN</td>
            <td>@string.Format("{0:dd-MMM-yyyy hh:mm tt}", g.Key.DATETIME)</td>
            <td>@g.Key.Dep_Name</td>
            <td>@g.Key.CreatedBy_Name</td>
            @*<td></td>*@
            <td>
                <table width="100%">
                    <thead>
                        <tr>
                            <th>Sr</th>
                            <th>Item Name</th>
                            <th>UOM</th>
                            <th>Qty</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{ int sr = 1;}
                        @foreach (var item in g)
                        {
                            <tr>
                                <td>@sr</td>
                                <td>@item.Complete_Name</td>
                                <td>@item.UOM</td>
                                <td>@item.Qty</td>
                            </tr>
                            sr++;
                        }
                    </tbody>
                </table>
            </td>
        </tr>
                    i++;
                }
            }
        }
    </tbody>
</table>
<script>
    $('#inv').DataTable();
</script>