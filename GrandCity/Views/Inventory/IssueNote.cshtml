                <tr style='height:25px;'>
                    <td class="s30">@i.</td>
                    <td class="s30" colspan="2">@a.SKU</td>
                    <td class="s30" colspan="15">@a.Complete_Name / @a.UOM </td>
                    <td class="s30" colspan="3">@a.Qty  </td>
                </tr>
                i++;
            }
            <tr style='height:25px;'>
                <td class="s30"></td>
