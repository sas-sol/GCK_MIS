    function updateTaxDetails() {
        
        //updating sellers tax details
        let SellerExempted = $('#ExemptBuyerTax').is(':checked');
        let BuyerExempted = $('#ExemptSellerTax').is(':checked');
        let buyExeRzn = $('.rzn-buy-exe').val();
        let selExeRzn = $('.rzn-slr-exe').val();

        let strc = ``;
        let buyStrc = ``;
        let ttlSels = $('.sel-inf-dfshjgk').length;
        let ttlBuyrs = $('.own-inf-kjhsdf').length;
        $('.sel-inf-dfshjgk').each(function (ind, val) {
            
            let nm = $(this).find('.Name').val();
            let flr = $(this).find('.filerCheck').is(':checked');

            if (nm == null || nm == '') {
                return;
            }

            let taxCalc = parseInt(RemoveComma($('.trnsfr-plt-prc').val()));
            if (SellerExempted) {
                strc += `<div class="col-md-6">
                        <label>${nm} (Tax Exempted):</label>
                    </div>

                    <div class="col-md-6">
                        <label><b>${buyExeRzn}</b></label>
                    </div>`;
            }
            else {
                if (flr) {
                    taxCalc = Math.ceil((taxCalc / ttlSels) * filerRate);
                }
                else {
                    taxCalc = Math.ceil((taxCalc / ttlSels) * non_filerRate);
                }

                strc += `<div class="col-md-6">
                        <label>${nm} :</label>
                    </div>

                    <div class="col-md-6">
                        <label><b>${taxCalc.toLocaleString()} </b></label>
                    </div>`;
            }
        });

        $('.sel-tax-dat').empty();
        $('.sel-tax-dat').append(strc);

        //plot-owns-fsdhjk
        $('.own-inf-kjhsdf').each(function (ind, val) {
            let nm = $(this).find('#Name').val();
            let flr = $(this).find('.filerCheck').is(':checked');
			let ttlOwnYrs = parseInt($(this).find('.ttl-yrs-own').val());

            if (nm == null || nm == '') {
                return;
            }

            if (BuyerExempted) {
                buyStrc += `<div class="col-md-6">
                        <label>${nm} (Tax Exempted) :</label>
                    </div>

                    <div class="col-md-6">
                        <label><b>${selExeRzn} </b></label>
                    </div>`;
            }
            else {
                if (ttlOwnYrs <= 3653) {

                    let taxCalc = parseInt(RemoveComma($('.trnsfr-plt-prc').val()));
                    if (flr) {
                        taxCalc = Math.ceil((taxCalc / ttlBuyrs) * filerRate);
                    }
                    else {
                        taxCalc = Math.ceil((taxCalc / ttlBuyrs) * non_filerRate);
                    }

                    buyStrc += `<div class="col-md-6">
                        <label>${nm} :</label>
                    </div>

                    <div class="col-md-6">
                        <label><b>${taxCalc.toLocaleString()} </b></label>
                    </div>`;
                }
                else {
                    buyStrc += `<div class="col-md-6">
                        <label>${nm} (Tax Exempted since own duration > 4yrs) :</label>
                    </div>

                    <div class="col-md-6">
                        <label><b>${0} </b></label>
                    </div>`;
                }
            }
        });

        $('.buy-tax-dat').empty();
        $('.buy-tax-dat').append(buyStrc);
    }

   

