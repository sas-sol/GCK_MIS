@model MeherEstateDevelopers.Models.TransferLetter
@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <style>
        body {
            font-family: Roboto,-apple-system,system-ui,BlinkMacSystemFont,Segoe UI,Oxygen,Ubuntu,Cantarell,Fira Sans,Droid Sans,Helvetica Neue,Arial,sans-serif;
        }

        @@media print {
            @@Page {
                size: A4
            }

            .logofooter {
                position: fixed;
                bottom: 0;
            }
        }
    </style>
</head>
<body>
    <div>
        <table cellspacing="0" border="0" style="width:1000px">
            <tr>
                <td width="200px" align="center" valign=bottom style="font-weight:bold"><b>Serial No.</b> @Model.Owner_Id</td>
                <td width="600px" align="center" valign=bottom style="font-weight:bold">
                    <img width="250" height="125" src="~/assets/static/images/SagVoucher/customer.jpg" />
                </td>
                <td width="200px" align="center" valign=bottom style="font-weight:bold">
                    <img style="float:right" src="~/assets/static/images/customer-copy.png" width=125 height=125>
                </td>
            </tr>
        </table>
        <table cellspacing="0" border="0" style="width:1000px">
            <tr>
                <td width="200px" align="center" valign=bottom style="font-weight:bold"></td>
                <td width="600px" align="center" valign=bottom style="font-weight:bold"><font size="6">Plot Transfer Letter</font></td>
                <td width="200px" align="center" valign=bottom style="font-weight:bold">
                </td>
            </tr>
        </table>
        <table cellspacing="0" border="0" style="width:1000px;font-size: 21px;margin-top: 50px;">
            <tr>
                <td align="left" valign=bottom style="line-height: 44px;">
                    <p>
                        <b>Plot No:</b>&nbsp;<u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.Plot_No&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>&nbsp;
                        <b>Block</b>&nbsp;<u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.Block&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>&nbsp;
                        <b>Size</b>&nbsp;<u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.Size&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>&nbsp;
                    </p>
                </td>
            </tr>
        </table>
        <table cellspacing="0" border="0" style="width:1000px;font-size: 19px;margin: 30px 0 30px 0">
            <tr>
                <td align="left" valign=bottom style="line-height: 44px;">
                    <p>
                        <b>Transferor Name:</b>&nbsp;<u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.Transfer_From&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>&nbsp;
                        <b>CNIC </b>&nbsp;<u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.T_CNIC&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>&nbsp;
                        @*<b>Mobile No</b>&nbsp;<u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.T_Mobile1&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>&nbsp;*@
                    </p>
                </td>
            </tr>
        </table>+
        <table cellspacing="0" border="0" style="width:1000px;font-size: 19px;margin: 30px 0 30px 0">
            <tr>
                <td align="left" valign=bottom style="line-height: 44px;">
                    <p>
                        <b>Transfer To:</b>&nbsp;<u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.Name&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>&nbsp;
                        @if (Model.IsCompanyProperty)
                        {
                            <b>NTN &nbsp;</b><u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.CNIC_NICOP&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>
                        }
                        else
                        {
                            <b>CNIC &nbsp;</b><u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.CNIC_NICOP&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>
                        }
                        <b>Mobile No</b>&nbsp;<u>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Model.Mobile_1&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</u>&nbsp;
                    </p>
                </td>
            </tr>
        </table>
        <table cellspacing="0" border="0" style="width:1000px;font-size: 22px;">
            <tr>
                <td width="300px" align="left" valign=bottom style="font-weight:bold;">Plot Total Value</td>
                <td width="180px" align="left" style="border-bottom:1px solid" valign=bottom> Rs. @string.Format("{0:n0}", Model.Total) /- </td>
                <td width="640"></td>
            </tr>
        </table>
        <table cellspacing="0" border="0" style="width:1000px;font-size: 22px;margin-top: 50px;">
            <tr>
                <td width="300px" align="left" valign=bottom style="font-weight:bold;">Received Amount</td>
                <td width="180px" align="left" style="border-bottom:1px solid" valign=bottom> Rs. @string.Format("{0:n0}", Model.Received) /- </td>
                <td width="640"></td>
            </tr>
        </table>
        <table cellspacing="0" border="0" style="width:1000px;font-size: 22px;margin-top: 50px;">
            <tr>
                <td width="300px" align="left" valign=bottom style="font-weight:bold;">Discount Amount</td>
                <td width="180px" align="left" style="border-bottom:1px solid" valign=bottom> Rs. @string.Format("{0:n0}", Model.Discount) /- </td>
                <td width="640"></td>
            </tr>
        </table>
        <table cellspacing="0" border="0" style="width:1000px;font-size: 22px;margin-top: 50px;">
            <tr>
                <td width="300px" align="left" valign=bottom style="font-weight:bold;">Outstanding Amount</td>
                <td width="180px" align="left" style="border-bottom:1px solid" valign=bottom> Rs. @string.Format("{0:n0}", Model.Total - Model.Discount - Model.Received) /- </td>
                <td width="640"></td>
            </tr>
        </table>
        <table cellspacing="0" border="0" style="width:1000px;font-size: 22px;margin-top:10%">
            <tr>
                <td width="350px" align="left" valign=bottom style="font-weight:bold;border-top:1px solid">Records Department</td>
                <td width="270px" align="right" valign=bottom></td>
                <td width="200px" align="right" valign=bottom style="font-weight:bold">Date</td>
                <td width="200px" align="center" valign=bottom style="border-bottom:1px solid;font-size: 17px;">@string.Format("{0:D}", Model.Transfer_Date)</td>
            </tr>
        </table>
        <table cellspacing="0" border="0" style="width:1000px;font-size: 22px;margin-top: 40%;">
            <tr>
                <td width="1000px" align="center" valign=bottom style="padding-right: 95px;">
                    <img src="~/assets/static/images/footer_urdu_logo.png" width=450 height=125>
                </td>
            </tr>
        </table>
    </div>
