    <table cellspacing="0" border="0" style="width:1000px">
        <tr>
            <td width="800px" align="right" valign=bottom style="padding-right: 95px;">
                @{
                    if (Model.Type == "Booking")
                    {
                        <img src="~/assets/static/images/FaisalBokReceipt/advance.png" width=375 height=125>
                    }
                    else if (Model.Type == "Advance")
                    {
                        <img src="~/assets/static/images/FaisalAdvReceipt/advance.png" width=375 height=125>
                    }
                    else if (Model.Type == "Installment")
                    {
                        <img src="~/assets/static/images/FaisalInstReceipt/advance.png" width=375 height=125>
                    }
                }
            </td>
            <td width="200px" align="right" valign=bottom>
                <img src="~/assets/static/images/customer-copy.png" width=125 height=125>
            </td>
        </tr>
    </table>
    <table cellspacing="0" border="0" style="width:1000px">
        <tr>
            <td width="250px"></td>
            <td width="90px" valign=bottom style="font-weight:bold">Receipt No.</td>
            <td width="125px" align="center" valign=bottom style="border-bottom:1px solid">@Model.ReceiptNo</td>
            <td width="246x" valign=bottom></td>
        </tr>
    </table>
    <table cellspacing="0" border="0" style="width:1000px">
        <tr>
            <td width="125px" align="left" valign=bottom style="font-weight:bold">For Payment of</td>
            <td width="850px" align="center" valign=bottom style="border-bottom:1px solid">@Model.Text</td>
        </tr>
    </table>
