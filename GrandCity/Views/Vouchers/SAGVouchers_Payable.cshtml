                <th width="50px"></th>
                <th width="50px"></th>
            </tr>
        </thead>
        <tbody>
            <tr style='height:18px;'>
                <td colspan="19" style="text-align:center">
                    <img width="200" height="100" src="~/assets/static/images/SagVoucher/customer.jpg" />
                </td>
            </tr>
            <tr style='height:18px;'>
                <td class="s5" colspan="09">@*@Model.Voucher.Name*@</td>
                <td class="s6" colspan="10">@paymentype Payment Voucher</td>
            </tr>
            <tr style='height:18px;'>
                <td class="s8" colspan="9">@*@Model.Voucher.Father_Name*@</td>
                <td class="s17" colspan="10" style="text-align:right">Voucher No: @Model.Voucher.VoucherNo</td>
            </tr>
            <tr style='height:18px;'>
                <td class="s12" colspan="9"> @*@Model.Voucher.Contact*@</td>
                <td class="s17" colspan="10" style="text-align:right">Voucher Date: @string.Format("{0:d}", Model.Voucher.DateTime)</td>
            </tr>
            <tr style='height:18px;'>
                <td colspan="19"></td>
            </tr>
            <tr style='height:23px;'>
                <td colspan="19"></td>
            </tr>
            <tr style='height:23px;'>
                <td colspan="19"></td>
            </tr>
            <tr style='height:30px;'>
                <td class="s27">Sr.</td>
                <td class="s27" colspan="11">DESCRIPTION</td>
                <td class="s28" colspan="2">Qty</td>
                <td class="s28" colspan="2">Rate</td>
                <td class="s28" colspan="3">Total</td>
            </tr>
            @{
                int count = 1;
                decimal? previouspaid = 0;
            }
            @foreach (var item in Model.Details)
            {
                <tr style='height:30px;'>
                    <td class="s30">@count.</td>
                    <td class="s30" colspan="11">@item.Head_Name</td>
                    <td class="s30" colspan="2">@String.Format("{0:n0}", item.Qty)</td>
                    <td class="s30" colspan="2">@String.Format("{0:n0}", item.Rate)</td>
                    <td class="s30" colspan="3">@string.Format("{0:n0}", item.Qty * item.Rate)</td>
                </tr>

                count++;
            }

            @if (paymentype != "Cash")
            {
                <tr style='height:30px;'><td class="s30"></td><td class="s30" colspan="10">@Model.Voucher.Bank - @Model.Voucher.Ch_Pay_Draft_No - @string.Format("{0:D}", Model.Voucher.Ch_Pay_Draft_Date)</td><td class="s30" colspan="2"></td><td class="s30" colspan="2"></td><td class="s30" colspan="2"></td><td class="s30" colspan="2"></td></tr>
            }
            <tr style='height:30px;'><td class="s30"></td><td class="s30" colspan="11" style="text-align:left">@Model.Voucher.Text</td><td class="s30" colspan="2"></td><td class="s30" colspan="2"></td><td class="s30" colspan="3"></td></tr>

            <tr style='height:37px;'>
                <td class="s41" colspan="14"><b>Amount in Words: </b>@Model.Voucher.AmountinWords</td>
                <td class="s48" colspan="2">Grand Total</td>
                <td class="s49" colspan="3"> Rs = @string.Format("{0:n0}", Model.paydet.Amount)/-  </td>
            </tr>

            <tr style='height:37px; color:indianred'>
                <td class="s41" colspan="14">@Model.Voucher.Description</td>
                <td class="s48" colspan="2">Payment Amount</td>
                <td class="s49" colspan="3"><span style="color:red;"> Rs = @string.Format("{0:n0}", Model.Voucher.Amount)/-  </span></td>
            </tr>

            @if (Model.VoucherList.Any())
            {
                <tr style="height:37px;">
                    <td class="s48" colspan="7"></td>
                    <td class="s49" colspan="5">Previous Vouchers</td>
                    <td class="s48" colspan="7"></td>
                </tr>
                foreach (var v in Model.VoucherList)
                {
                    if (v.PaymentType != "Cash")
                    {
                        <tr style='height:30px;'><td class="s30">@count.</td><td class="s30" colspan="10" style="text-align:right;">@v.VoucherNo - @v.PaymentType - @v.Ch_Pay_Draft_No - @String.Format("{0: dd/MM/yyyy}", v.DateTime) </td><td class="s30" colspan="2"></td><td class="s30" colspan="2"></td><td class="s30" colspan="2"></td><td class="s30" colspan="2">@string.Format("{0:n0}", v.Amount)</td></tr>
                        previouspaid = previouspaid + v.Amount;
                    }
                    else
                    {
                        <tr style='height:30px;'>
                            <td class="s41" colspan="6"></td>
                            <td class="s41" colspan="2" style="text-align:center;">@String.Format("{0: dd/MM/yyyy}", v.DateTime)</td>
                            <td class="s41" colspan="4" style="text-align:center;">@v.VoucherNo</td>
                            <td class="s41" colspan="2" style="text-align:right;">Rs = @string.Format("{0:n0}", v.Amount)</td>
                            <td class="s41" colspan="5"></td>
                        </tr>
                        previouspaid = previouspaid + v.Amount;
                    }
                }
                <tr style='height:37px;'>
                    <td class="s48" colspan="6"></td>
                    <td class="s48" colspan="5">Previous Paid Total</td>
                    <td class="s49" colspan="3" style="text-align:right;"> Rs = @string.Format("{0:n0}", previouspaid)/- </td>
                    <td class="s48" colspan="5"></td>
                </tr>
            }
            <tr style='height:37px;'>
                <td class="s48" colspan="6"></td>
                <td class="s48" colspan="5">Pending Amount</td>
                <td class="s49" colspan="3"> Rs = @string.Format("{0:n0}", Model.paydet.Remaining)/-  </td>
                <td class="s48" colspan="5"></td>
            </tr>
            <tr style='height:60px;'>
                <td colspan="21"></td>
            </tr>
            <tr style='height:18px;'>
                <td colspan="21"></td>
            </tr>
            <tr style='height:18px;'>
                <td colspan="21"></td>
            </tr>
        </tbody>
            <tfoot>
                <tr>
                    <th colspan="4" style="text-align:center;width:250px">Prepared By<br /> @Model.Voucher.Pre_Name</th>
                    <th colspan="4" style="text-align:center;width:250px">Supervised By<br />@Model.Voucher.Sup_Name</th>
                    <th colspan="4" style="text-align:center;width:250px">Paid By<br />@Model.Voucher.Pre_Name</th>
                    <th colspan="4" style="text-align:center;width:250px"> </th>
                    <th colspan="4" style="text-align:center;width:250px">Received By <br />@Model.Voucher.Name</th>
                </tr>
            </tfoot>
        </table>
</div> 