@model IEnumerable<MeherEstateDevelopers.Models.OverdueQualifying_Shops>
<div class="col-md-12 ovd-blk-plt">
    <ul class="nav nav-tabs" id="myTab" role="tablist">
        @foreach (var g in Model.GroupBy(x => x.Project))
        {
            <li class="nav-item"><a class="nav-link" role="tab" data-toggle="tab" data-lead="1" aria-controls="home" aria-selected="true" href="#@g.Key.Replace(" ","-")">@g.Key</a></li>
        }
    </ul>
    <div class="tab-content">
        @foreach (var g in Model.GroupBy(x => x.Project))
        {
            <div id="@g.Key.Replace(" ","-")" class="tab-pane">
                <table class="table table-striped table-bordered" id="ovde" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>Sr.</th>
                            <th>Name</th>
                            <th>Unit Number</th>
                            <th>Floor</th>
                            <th>Type</th>
                            <th>Area</th>
                            <th>Project</th>
                            <th>Overdue Amount</th>
                            <th>Pending Installments</th>
                            <th>Mobile 1</th>
                            <th>Verification</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            if (Model.Any())
                            {
                                int dealercounter = 1;
                                foreach (var item in g)
                                {
                                    <tr class="pointer file-folo-det" id="@item.Id">
                                        <td>@dealercounter</td>
                                        <td class="name">@item.Name <i style="float:right; z-index:1111111" class="ti-info-alt cust-com-info" data-toggle="modal" data-target="#Modal"></i></td>
                                        <td class="file-num">@item.Shop_No</td>
                                        <td>@item.Floor</td>
                                        <td>@item.Type</td>
                                        <td>@item.Area</td>
                                        <td>@item.Project</td>
                                        <td>@string.Format("{0:n0}", Math.Round(Convert.ToDecimal(item.OverDueAmount)))</td>
                                        <td>@item.Installments</td>
                                        <td>@item.Mobile_1</td>
                                        <td>
                                            <button class="btn btn-primary fir-war-com" data-id="@item.Id" data-ownid="@item.Owner_Id" style="font-size:13px;padding: 3px;">Warning Letter</button>
                                            <button class="btn btn-primary can-let-com-con" data-id="@item.Id" data-ownid="@item.Owner_Id" style="font-size:13px;padding: 3px;">Cancellation Letter For Non Paid Confirmation</button>
                                        </td>
                                        <td class="col-md-2">
                                            <div class="form-group ">
                                                <select class="form-control select1 Qualify-war-com-shop" name="Type" id="Type" data-ownid="@item.Owner_Id" data-id="@item.Id" data-let="1">
                                                    <option value="">Select One</option>
                                                    <option value="First">First Warning</option>
                                                    <option value="Second">Second Warning</option>
                                                    <option value="Last">Cancelled</option>
                                                </select>
                                            </div>
                                        </td>
                                    </tr>
                                    dealercounter++;
                                }
                            }
                        }
                    </tbody>
                </table>
            </div>
        }
    </div>
</div>
<style>
    table tr th, table tr td {
        padding: 5px !important;
        font-size: 12px;
    }

    .slider:before {
        height: 13px !important;
        width: 13px !important;
    }
</style>
<script>
    $(document).ready(function () {
        $(".ovd-blk-plt .nav .nav-item .nav-link").first().addClass("active");
        $(".ovd-blk-plt .tab-pane").first().addClass("active");
    });
</script>


