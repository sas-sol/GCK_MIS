                                @{
                                    if (ViewBag.From == ViewBag.To)
                                    {
                                        <span>@string.Format("{0:D}", ViewBag.From)</span>
                                    }
                                    else
                                    {
                                        <span>@string.Format("{0:D}", ViewBag.From) - @string.Format("{0:D}", ViewBag.To)  </span>
                                    }
                                }


                            </th>
                        </tr>
