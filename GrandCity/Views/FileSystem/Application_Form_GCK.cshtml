            <td width="150px" align="left" valign=bottom style="font-size:small;"><input type="checkbox" /><b>NORMAL</b><br /><span style="font-size:x-small">&nbsp;</span></td>
            <td width="150px" align="left" valign=bottom style="font-size:small;"><input type="checkbox" /><b>CORNER</b><br /><span style="font-size:x-small">(10% Extra Charges)</span></td>
            <td width="200px" align="left" valign=bottom style="font-size:small"><input type="checkbox" /><b>MAIN ROAD</b><br /><span style="font-size:x-small">(10% Extra Charges)</span></td>
            <td width="200px" align="left" valign=bottom style="font-size:small"><input type="checkbox" /> <b>FACING PARK</b><br /><span style="font-size:x-small">(10% Extra Charges)</span></td>
