@model MeherEstateDevelopers.Models.Sp_Get_FileFormData_Result
@if (Model != null)
{
    <input type="hidden" value="@Model.Id" id="PlotId" name="PlotId" />
    <input type="hidden" value="FileManagement" name="Module" id="Module" />
    <div class="bgc-white p-20 bd">
        <div class="form-row row">
            <div class="col-md-12">
                <div class="form-row">
                    <div class="form-group col-md-2">
                        <label>File No.</label>
                        <h5 id="plt-no">@Model.FileFormNumber</h5>
                    </div>
                    <div class="form-group col-md-2">
                        <label>Block</label>
                        <h5 id="plt-blk">@Model.Block</h5>
                    </div>
                    <div class="form-group col-md-2">
                        <label>Size</label>
                        <h5 id="plt-size">@Model.Plot_Size</h5>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Name</label>
                        <h5 id="nam">@Model.Name</h5>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Father Name</label>
                        <h5 id="nam">@Model.Father_Husband</h5>
                    </div>
                </div>
            </div>
        </div>
    </div>

}
else
{
    <div class="bgc-white p-20 bd">
        <h5 class="c-grey-900">No Record Found</h5>
    </div>
}
