@model IEnumerable<MeherEstateDevelopers.Models.BallotPlot>
<style>
    .ul {
        padding: 0;
        display: block;
        margin: 0 auto;
        width: 95%;
    }

    .li {
        display: block;
        height: 85px;
        width: 100%;
        border-radius: 13px;
        background-color: #20448b;
        animation-name: animateIn;
        animation-duration: 150ms;
        animation-delay: calc(var(--animation-order) * 200ms);
        animation-fill-mode: both;
        animation-timing-function: ease-in-out;
    }

        .li + .li {
            margin-top: 16px;
        }

    @@keyframes animateIn {
        0% {
            opacity: 0;
            transform: scale(0.6) translateY(-8px);
        }

        100% {
            opacity: 1;
        }
    }
</style>
<ul class="ul">
    @foreach (var a in Model)
    {
    <li class="li" style="--animation-order: 6;font-size:40px;text-align:center;color:white;display:flex">
        <span style=" min-width: 350px; max-width: 400px;">
            @a.BallotFile
        </span>
        <span style=" min-width: 150px; max-width: 100px;">
            @a.PlotNo
        </span>
        <span style=" min-width: 50px; max-width: 50px;">
            @a.Sector
        </span>
        <span style=" min-width: 300px; max-width: 400px;">
            @a.PlotType
        </span>
        <span style=" min-width: 100px; max-width: 200px;">
            @a.Actual_Size Marla
        </span>
    </li>
    }
</ul>
<script>
    $(document).ready(function () {

        window.setTimeout(function () {
                $("html, body").animate({ scrollTop: $(document).height() }, 90000);
        }, 3000);
    });
</script>

