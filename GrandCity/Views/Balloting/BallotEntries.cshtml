@model IEnumerable<MeherEstateDevelopers.Models.Tempopp>
@{
    if (User.IsInRole("Administrator"))
    {
        Layout = "~/Views/Shared/_AdminLayout.cshtml";
    }
    else if (User.IsInRole("CEO"))
    {
        Layout = "~/Views/Shared/_CEOLayout.cshtml";
    }
    else if (User.IsInRole("Staff"))
    {
        Layout = "~/Views/Shared/_StaffLayout.cshtml";
    }
}
<div class="col-md-12">
    <div class="bgc-white bd bdrs-3 p-20 mB-20">
        <h4 class="c-grey-900 mB-20">Plots Transfer Requests</h4>
        <table class="translst table table-striped table-bordered" cellspacing="0" style="width:100%">
            <thead class="thead-dark">
                <tr>
                    <th>Sr.</th>
                    <th>File#</th>
                    <th>Owner Name</th>
                    <th>Entrance Time</th>
                </tr>
            </thead>
            <tbody>
                @{
                    if (Model.Any())
                    {
                        int srcounter = 1;



                        foreach (var item in Model)
                        {
                            
                           
                            <tr>
                                <td>@(srcounter++)</td>
                                <td>@item.FileNum</td>
                                <td>@item.OwnerName</td>
                                <td>@item.EntryTime</td>
                            </tr>
                            srcounter++;
                        }
                    }
                }
            </tbody>
        </table>
    </div>
</div>
<script>
    $(function () {
        $('.translst').dataTable();
    })
</script>