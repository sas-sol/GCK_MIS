@model IEnumerable<MeherEstateDevelopers.Models.RecoveryReport>
@{ if (User.IsInRole("Administrator"))
    {
        Layout = "~/Views/Shared/_AdminLayout.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_AdminLayout.cshtml";
    }
}

<div class="row">
    <div class="col-md-12">
        <div class="bdrs-3 ov-h bgc-white p-10 bd">
            <div class="form-row">
                <h5>Custome Search Filters</h5>
            </div>
            <div class="form-row">
                <div class="form-group col-md-2">
                    <label>From</label>
                    <input type="text" id="from" class="form-control" data-provide="datepicker" />
                </div>
                <div class="form-group col-md-2">
                    <label>To</label>
                    <input type="text" id="to" class="form-control" data-provide="datepicker" />
                </div>
                
                <div class="form-group col-md-2">
                    <button style="margin-top:29px" class="btn btn-info rec-sea-res-Pend">Search</button>
                </div>
                <div class="form-group col-md-2">
                    <label style="margin-top:29px">Export Option</label>
                    <a href="#" class="btn btn-primary" style="margin-top:29px;float:right;color:white !important;" id="test" onClick="javascript:fnExcelReport('Custom Report');">Excel</a>
                    @*<a href="#" class="btn btn-primary" style="margin-top:29px;margin-right:5px;float:right;color:white !important;" onClick="convert()">PDF</a>*@
                </div>
            </div>
            <div class="form-row">
                <div class="table-responsive" id="report"></div>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).on("click", ".rec-sea-res-Pend", function (e) {
        var from = $("#from").val();
        var to = $("#to").val();
        var user = $("#users").val();
        $("#report").load("/Finance/PendingReceiptSearchResult/", { From: from, To: to });
    });


