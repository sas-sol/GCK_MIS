@model IEnumerable<MeherEstateDevelopers.Models.Sp_Get_PropertyDeal_CommissionReq_Result>
@{ Layout = "~/Views/Shared/_StaffLayout.cshtml"; }
<div class=" bgc-white bd bdrs-3 p-20 mB-20 row">
    <div class="col-md-12">
        <h5>Commission Approval</h5>
        <table id="allleads" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead class="thead-dark">
                <tr>
                    <th>Sr.</th>
                    <th>Name</th>
                    <th>File/Plot</th>
                    <th>Block</th>
                    <th>Type</th>
                    <th>Commission</th>
                    <th>Commission Rate</th>
                    <th>Requested By</th>
                </tr>
            </thead>
            <tbody>
                @{
                    if (Model.Any())
                    {
                        int srcounter = 1;
                        foreach (var item in Model)
                        {
                            <tr class="pointer deal-dets" id="@item.Id">
                                <td>@srcounter</td>
                                <td>@item.Name</td>
                                <td>@item.File_Plot_Number</td>
                                <td>@item.Block</td>
                                <td>@item.Type</td>
                                <td>@string.Format("{0:n0}",item.Commession)</td>
                                <td>@item.Commission_Type</td>
                                <td>@item.RequestedBy</td>
                            </tr>
                            srcounter++;
                        }
                    }
                }
            </tbody>
        </table>
    </div>
</div>
<script>
    $(document).ready(function () {
        $('#allleads').DataTable({
        });
    });

</script>