@model IEnumerable<MeherEstateDevelopers.Models.LeaveRequisitionModel>
<h4 class="c-grey-900 mB-20">Leaves</h4>
<table id="Manager_lvl_leaves_show" class="table table-striped table-bordered " cellspacing="0" width="100%">
    <thead class="thead-dark">
        <tr>
            <th>Employee Name</th>
            <th>Leave Type</th>
            <th>Reason</th>
            <th>Cancel</th>
            <th>Official Leaves</th>
            <th>Approve</th>
            @*<th>Approve</th>*@
            @*<th>Time</th>
                <th>Activity</th>*@
        </tr>
    </thead>
    <tbody>
        @{
            if (Model.Any())
            {
                foreach (var item in Model)
                {
                    <tr class="pointer " id="@item.Id">
                        <td>@item.UserName</td>
                        <td>@item.Name</td>
                        <td>@item.Reason</td>
                        <td>@item.Cancel</td>
                        <td>@item.OfficalLeave</td>
                        <td>  <input type="checkbox" id="chk_app" name="approve" value="approve"></td>
                        @*<td><i class="ti-check-box " id=""></i></td>*@

                        @*<td><i class="ti-check on_leave_tick" i></i></td>*@

                        @*<td>@item.datesele</td>
                            <td>@item.timeselec</td>
                            <td>@item.Activity</td>*@
                        @*<td><i class="ti-pencil comp-assets" id="@item.Id"></i></td>*@

                    </tr>
                }
            }
        }
    </tbody>
</table>